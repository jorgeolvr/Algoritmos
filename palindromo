import java.util.Scanner;

public class Palindromo {
	public static boolean isPalindromo(String palavra) {
		boolean palindromo = true;
		
		for(int i = 0; i < (palavra.length() / 2); i++) {
			if(palavra.charAt(i) != palavra.charAt(palavra.length() - 1 - i)) {
				palindromo = false;
			}
		}
		return palindromo;
	}

	public static Boolean encerramento(String parada) {
        Boolean condicao = false;

        if (parada.charAt(0) == 'F' && parada.charAt(1) == 'I' && parada.charAt(2) == 'M') {
            condicao = true;
        } 
        return condicao;
    } 

	public static void main(String[] args) throws Exception {
		int num = 0;
		String[] entrada = new String[1000];
      Scanner scanner = new Scanner(System.in);
      	
		do {
        	entrada[num] = scanner.nextLine();
      	} while (encerramento(entrada[num++]) == false); num--;

		for (int i = 0; i < num ; i++) {
			boolean resultado = isPalindromo(entrada[i].toLowerCase().replaceAll(" ","").replace(",", ""));
			if(resultado) {
				System.out.println("SIM");
			} else {
				System.out.println("NAO");
			}
		}
	}
}
